---
// Supports weights 300-800
import "@fontsource-variable/open-sans";
import "@fontsource/zilla-slab/300.css";
import "@fontsource/zilla-slab/600.css";

import opensans from "@fontsource-variable/open-sans/files/open-sans-latin-wght-normal.woff2";
import opensansext from "@fontsource-variable/open-sans/files/open-sans-latin-ext-wght-normal.woff2";
import zilla300 from "@fontsource/zilla-slab/files/zilla-slab-latin-300-normal.woff2";
import zilla600 from "@fontsource/zilla-slab/files/zilla-slab-latin-600-normal.woff2";
import zilla300ext from "@fontsource/zilla-slab/files/zilla-slab-latin-ext-300-normal.woff2";
import zilla600ext from "@fontsource/zilla-slab/files/zilla-slab-latin-ext-600-normal.woff2";
---

<link rel="preload" as="font" href={opensans} type="font/woff2" crossorigin />
<link rel="preload" as="font" href={opensansext} type="font/woff2" crossorigin />
<link rel="preload" as="font" href={zilla300} type="font/woff2" crossorigin />
<link rel="preload" as="font" href={zilla600} type="font/woff2" crossorigin />
<link rel="preload" as="font" href={zilla300ext} type="font/woff2" crossorigin />
<link rel="preload" as="font" href={zilla600ext} type="font/woff2" crossorigin />


---
import { getEntry, render } from "astro:content";
import type { CollectionKey } from "astro:content";

interface Props {
	collection: CollectionKey;
	entrySlug: string;
}
const { collection, entrySlug } = Astro.props;

const collectionEntry = await getEntry(collection, entrySlug);
if (!collectionEntry) {
	throw new Error(`Entry not found for ${collection}/${entrySlug}`);
}
const { Content } = await render(collectionEntry);
---

<Content />
